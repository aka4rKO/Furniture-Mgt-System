<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lk.UGNiP.OfficeMoveController">
   <children>
      <Label layoutX="260.0" layoutY="14.0" text="Office Move">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <Button layoutX="14.0" layoutY="361.0" mnemonicParsing="false" onAction="#maintenanceCall" text="Maintenance Call" />
      <ListView fx:id="movableList" layoutX="14.0" layoutY="95.0" prefHeight="200.0" prefWidth="200.0" />
      <Button layoutX="265.0" layoutY="235.0" mnemonicParsing="false" onAction="#addItem" text="Add items" />
      <Button layoutX="255.0" layoutY="270.0" mnemonicParsing="false" onAction="#removeItem" text="Remove items" />
      <Button layoutX="493.0" layoutY="306.0" mnemonicParsing="false" onAction="#requestMove" text="Request move" />
      <Label layoutX="14.0" layoutY="70.0" text="Movable Items" />
      <Label layoutX="464.0" layoutY="70.0" text="Items selected to move" />
      <ListView fx:id="selectedList" layoutX="386.0" layoutY="95.0" prefHeight="200.0" prefWidth="200.0" />
      <ComboBox fx:id="floor" layoutX="247.0" layoutY="146.0" onAction="#floorSelected" prefHeight="25.0" prefWidth="108.0" promptText="Floor" />
      <ComboBox fx:id="dept" layoutX="247.0" layoutY="111.0" onAction="#deptSelected" prefHeight="25.0" prefWidth="108.0" promptText="Department" />
      <Button fx:id="generateReportBtn" layoutX="452.0" layoutY="361.0" mnemonicParsing="false" onAction="#generateReport" text="Generate FMO Report" />
      <Button layoutX="527.0" layoutY="12.0" mnemonicParsing="false" onAction="#logout" text="Log Out" />
      <ComboBox fx:id="room" layoutX="261.0" layoutY="183.0" onAction="#roomSelected" prefHeight="25.0" prefWidth="79.0" promptText="Room" />
   </children>
</AnchorPane>
