<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="250.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lk.UGNiP.MaintenanceCallController">
   <children>
      <Button layoutX="14.0" layoutY="211.0" mnemonicParsing="false" onAction="#officeMove" text="Back" />
      <Label layoutX="123.0" layoutY="14.0" text="Maintenance Call">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <CheckBox fx:id="offOpen" layoutX="113.0" layoutY="78.0" mnemonicParsing="false" onAction="#offOpenBox" text="Office opened" />
      <CheckBox fx:id="lightReplace" layoutX="113.0" layoutY="102.0" mnemonicParsing="false" onAction="#lightReplaceBox" text="Lighting replacement" />
      <CheckBox fx:id="carpetClean" layoutX="113.0" layoutY="127.0" mnemonicParsing="false" onAction="#carpetCleanBox" text="Carpet cleaned" />
      <Button fx:id="action" layoutX="313.0" layoutY="211.0" mnemonicParsing="false" onAction="#actionMethod" text="Action Call" />
      <Button fx:id="log" layoutX="247.0" layoutY="211.0" mnemonicParsing="false" onAction="#logMethod" text="Log Call" />
      <Button fx:id="cancel" layoutX="163.0" layoutY="211.0" mnemonicParsing="false" onAction="#cancelMethod" text="Cancel Call" />
   </children>
</AnchorPane>
